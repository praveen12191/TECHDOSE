class Solution:
    def singleNumber(self, nums: List[int]) -> int:
        ans = 0 
        for i in range(0,32):
            ctr=0
            for j in nums:
                if(j&(1<<i)):
                    ctr+=1 
            if(ctr%3):
                ans |= 1<<i
        if(ans>2**31-1):
            return ans-2**32
        else:
            return ans
            